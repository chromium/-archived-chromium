Import('env')

env = env.Clone()

if env.Bit('windows'):
  env.Append(
    CCFLAGS = [
      '/TP',
      
      '/wd4138',
      '/wd4244',
      '/wd4291',
      '/wd4305',
      '/wd4521',
      '/wd4099',
      '/wd4800',
    ],
    CPPDEFINES = [
      '__STD_C',
    ],
  )

if env.Bit('posix'):
  if '-Wall' in env['CXXFLAGS'] and '-Werror' in env['CXXFLAGS']:
    # It'd be nice to fix these warnings upstream, but we're not going to
    # block on it.
    env['CXXFLAGS'].remove('-Werror')

env.Append(
    CPPPATH = [
        '$THIRD_PARTY_DIR/sqlite/preprocessed'
    ]
)

