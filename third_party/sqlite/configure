  XTHREADCONNECT=''
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
else
  XTHREADCONNECT='-DSQLITE_ALLOW_XTHREAD_CONNECT=1'
  echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
fi


